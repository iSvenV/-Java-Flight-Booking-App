<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="455.0" prefWidth="374.0" style="-fx-background-color: #33adff;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1.Controller.FlightEdit">
    <children>
        <AnchorPane layoutX="14.0" layoutY="14.0" prefHeight="431.0" prefWidth="345.0" style="-fx-background-color: white; -fx-border-color: black;">
            <children>
                <Label layoutX="14.0" layoutY="14.0" prefHeight="24.0" prefWidth="155.0" text="Edit Flight No." textFill="#33adff">
                    <font>
                        <Font name="Verdana Bold" size="19.0" />
                    </font>
                </Label>
                <Label layoutX="23.0" layoutY="76.0" prefHeight="17.0" prefWidth="69.0" text="ID:">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Label>
                <Label layoutX="23.0" layoutY="110.0" prefHeight="17.0" prefWidth="69.0" text="From:">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Label>
                <Label layoutX="23.0" layoutY="144.0" prefHeight="17.0" prefWidth="69.0" text="To:">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Label>
                <Label layoutX="23.0" layoutY="178.0" prefHeight="17.0" prefWidth="69.0" text="Date:">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Label>
                <Label layoutX="23.0" layoutY="212.0" prefHeight="17.0" prefWidth="69.0" text="Time:">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Label>
                <Label layoutX="23.0" layoutY="246.0" prefHeight="17.0" prefWidth="110.0" text="Flight Length:">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Label>
                <Label layoutX="23.0" layoutY="280.0" prefHeight="17.0" prefWidth="93.0" text="Current Status:">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Label>
                <TextField fx:id="inputID" layoutX="178.0" layoutY="72.0" prefHeight="25.0" prefWidth="140.0" />
                <TextField fx:id="inputFrom" layoutX="178.0" layoutY="106.0" prefHeight="25.0" prefWidth="140.0" />
                <TextField fx:id="inputTo" layoutX="178.0" layoutY="140.0" prefHeight="25.0" prefWidth="140.0" />
                <TextField fx:id="inputLength" layoutX="178.0" layoutY="242.0" prefHeight="25.0" prefWidth="140.0" />
                <Label fx:id="checker" alignment="CENTER" layoutX="29.0" layoutY="49.0" prefHeight="17.0" prefWidth="292.0" textFill="RED" />
                <Button fx:id="buttonApply" layoutX="177.0" layoutY="387.0" mnemonicParsing="false" onAction="#pressedApply" prefHeight="23.0" prefWidth="61.0" style="-fx-background-color: #33adff;" text="Apply" textFill="WHITE">
                    <font>
                        <Font name="Verdana Bold" size="12.0" />
                    </font>
                </Button>
                <Button fx:id="buttonBack" layoutX="107.0" layoutY="387.0" mnemonicParsing="false" onAction="#pressedBack" prefHeight="23.0" prefWidth="61.0" style="-fx-background-color: #33adff;" text="Back" textFill="WHITE">
                    <font>
                        <Font name="Verdana Bold" size="12.0" />
                    </font>
                </Button>
                <RadioButton fx:id="openBox" layoutX="179.0" layoutY="280.0" mnemonicParsing="false" onAction="#selectedBox" prefHeight="17.0" prefWidth="140.0" text="Open">
                    <font>
                        <Font name="System Bold Italic" size="12.0" />
                    </font>
                    <toggleGroup>
                        <ToggleGroup fx:id="status" />
                    </toggleGroup>
                </RadioButton>
                <RadioButton fx:id="canceledBox" layoutX="179.0" layoutY="301.0" mnemonicParsing="false" onAction="#selectedBox" prefHeight="17.0" prefWidth="140.0" text="Canceled" toggleGroup="$status">
                    <font>
                        <Font name="System Bold Italic" size="12.0" />
                    </font>
                </RadioButton>
                <RadioButton fx:id="airborneBox" layoutX="179.0" layoutY="323.0" mnemonicParsing="false" onAction="#selectedBox" prefHeight="17.0" prefWidth="140.0" text="Airborne" toggleGroup="$status">
                    <font>
                        <Font name="System Bold Italic" size="12.0" />
                    </font>
                </RadioButton>
                <RadioButton fx:id="landedBox" layoutX="179.0" layoutY="345.0" mnemonicParsing="false" onAction="#selectedBox" prefHeight="17.0" prefWidth="140.0" text="Landed" toggleGroup="$status">
                    <font>
                        <Font name="System Bold Italic" size="12.0" />
                    </font>
                </RadioButton>
                <TextField fx:id="inputHour" layoutX="178.0" layoutY="208.0" prefHeight="25.0" prefWidth="69.0" promptText="Hour" />
                <TextField fx:id="inputMinute" layoutX="248.0" layoutY="208.0" prefHeight="25.0" prefWidth="69.0" promptText="Minute" />
                <TextField fx:id="inputMonth" layoutX="177.0" layoutY="174.0" prefHeight="25.0" prefWidth="69.0" promptText="Month" />
                <TextField fx:id="inputDay" layoutX="247.0" layoutY="174.0" prefHeight="25.0" prefWidth="69.0" promptText="Day" />
            <Label fx:id="titleLabel" layoutX="171.0" layoutY="14.0" prefHeight="24.0" prefWidth="155.0" textFill="#33adff">
               <font>
                  <Font name="Verdana Bold" size="19.0" />
               </font>
            </Label>
            <Label fx:id="appliedLabel" alignment="CENTER" layoutX="29.0" layoutY="49.0" prefHeight="17.0" prefWidth="292.0" text="changes applied!" textFill="#26e646" />
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
