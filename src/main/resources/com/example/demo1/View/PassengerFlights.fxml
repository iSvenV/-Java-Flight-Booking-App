            <TextField fx:id="inputTicketNum" layoutX="395.0" layoutY="348.0" prefHeight="30.0" prefWidth="149.0" promptText="number of tickets" />
            <ListView fx:id="priceList" layoutX="553.0" layoutY="113.0" prefHeight="220.0" prefWidth="86.0" style="-fx-border-color: black;" />
            <Label alignment="CENTER" layoutX="553.0" layoutY="92.0" prefHeight="21.0" prefWidth="86.0" style="-fx-background-color: gray; -fx-border-color: black;" text="Price" />
            <Label fx:id="checker" alignment="CENTER" layoutX="26.0" layoutY="53.0" prefHeight="17.0" prefWidth="601.0" textFill="RED" />
            <Label fx:id="purchaseLabel" alignment="CENTER" layoutX="28.0" layoutY="53.0" prefHeight="17.0" prefWidth="588.0" text="ticket(s) purchased successfully!" textFill="#1af434" />
