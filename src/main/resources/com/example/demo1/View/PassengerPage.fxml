            <Button fx:id="ticketsButton" layoutX="131.0" layoutY="203.0" mnemonicParsing="false" onAction="#pressedTickets" prefHeight="37.0" prefWidth="203.0" style="-fx-background-color: #33adff;" text="VIEW RESERVED TICKETS" textFill="WHITE">
