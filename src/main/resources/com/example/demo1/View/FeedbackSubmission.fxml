<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="520.0" style="-fx-background-color: #33adff;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1.Controller.FeedbackSubmission">
    <children>
        <AnchorPane layoutX="19.0" layoutY="18.0" prefHeight="363.0" prefWidth="481.0" style="-fx-background-color: white; -fx-border-color: black;">
            <children>
                <Label layoutX="21.0" layoutY="14.0" prefHeight="30.0" prefWidth="305.0" text="Feedback Submission" textFill="#33adff">
                    <font>
                        <Font name="Verdana Bold" size="24.0" />
                    </font>
                </Label>
                <Button fx:id="backButton" layoutX="160.0" layoutY="313.0" mnemonicParsing="false" onAction="#pressedBack" prefHeight="30.0" prefWidth="76.0" style="-fx-background-color: #33adff;" text="BACK" textFill="WHITE">
                    <font>
                        <Font name="Verdana Bold Italic" size="12.0" />
                    </font>
                </Button>
                <Button fx:id="sendButton" layoutX="245.0" layoutY="313.0" mnemonicParsing="false" onAction="#pressedSend" prefHeight="30.0" prefWidth="76.0" style="-fx-background-color: #33adff;" text="SEND" textFill="WHITE">
                    <font>
                        <Font name="Verdana Bold Italic" size="12.0" />
                    </font>
                </Button>
                <Label alignment="CENTER" layoutX="21.0" layoutY="50.0" prefHeight="17.0" prefWidth="401.0" text="submit your feedback to help us improve the system!">
                    <font>
                        <Font name="Verdana Bold" size="13.0" />
                    </font>
                </Label>
                <TextField fx:id="input" alignment="TOP_LEFT" layoutX="50.0" layoutY="125.0" prefHeight="155.0" prefWidth="381.0">
               <font>
                  <Font name="System Italic" size="12.0" />
               </font></TextField>
                <Label fx:id="checker" alignment="CENTER" layoutX="140.0" layoutY="288.0" prefHeight="17.0" prefWidth="201.0" textFill="#1af42f" />
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
